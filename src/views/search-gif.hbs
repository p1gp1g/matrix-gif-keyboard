<div class="pb-4">
  <div class="mt-1 flex rounded-md items-center justify-center">
    <div class="relative text-gray-600 focus-within:text-gray-800 bg-gray-600 rounded-md border-0 basis-full lg:basis-2/3 xl:basis-1/3">
      <form id="search" action="">
      <span class="absolute inset-y-0 right-0 flex pr-2 bg-gray-600 rounded-md">
        <a class="p-1 focus:outline-none focus:shadow-outline bg-gray-600 text-gray-400 text-sm">
          <img id="service" class="bg-gray-600 pt-{{padding}}" alt="{{service}}" width="60px" src="powered_{{service}}.png"/>
        </a>
      </span>
      <input type="" id="q" class="w-full py-2 text-sm text-white bg-gray-600 rounded-md pl-10 focus:outline-none focus:text-white outline-none" placeholder="Search GIFs" autocomplete="off">
      <span class="absolute inset-y-0 left-0 flex pl-2 bg-gray-600 rounded-md">
        <button class="p-1 focus:outline-none focus:shadow-outline text-gray-400 text-sm" id="searchbtn" type="submit">
          <svg fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="1" viewBox="0 0 24 24" class="w-5 h-5 bg-gray-600"><path d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z"></path></svg>
        </button>
      </span>
      </form>
      </div>
    </div>
  </div>
<ul id="results" role="list" class="grid grid-cols-3 gap-x-4 gap-y-8 sm:grid-cols-3 sm:gap-x-6 lg:grid-cols-4 xl:gap-x-8">
</ul>